{
  "expo": {
    "name": "BuyForce",
    "slug": "buyforce",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",

    "platforms": ["ios", "android", "web"],

    "jsEngine": "jsc",

    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#4630eb"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.buyforce.app"
    },

    "web": {
      "bundler": "metro"
    }
  }
}
